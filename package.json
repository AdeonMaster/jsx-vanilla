{
  "name": "jsx-vanilla",
  "version": "3.1.2",
  "description": "JSX is a JavaScript XML/HTML syntax extender, that allows you to render XML/HTML tags inside vanilla JavaScript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdeonMaster/jsx-vanilla.git"
  },
  "homepage": "https://github.com/AdeonMaster/jsx-vanilla#readme",
  "bugs": {
    "url": "https://github.com/AdeonMaster/jsx-vanilla/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib --ignore src/__tests__/",
    "build": "npm run eslint && npm run build:lib && npm run jest",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "jest": "jest"
  },
  "keywords": [
    "html",
    "js",
    "preprocessor",
    "jsx",
    "acorn"
  ],
  "author": "AdeonMaster",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "prettier": "2.0.5",
    "regenerator-runtime": "^0.13.5",
    "jest": "^26.1.0"
  },
  "dependencies": {
    "acorn": "^7.3.1",
    "acorn-jsx": "^5.2.0",
    "acorn-walk": "^7.2.0"
  }
}
