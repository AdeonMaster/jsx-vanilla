{
  "name": "jsx-vanilla",
  "version": "3.1.0",
  "description": "JSX is a JavaScript XML/HTML syntax extender, that allows you to render XML/HTML tags inside vanilla JavaScript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdeonMaster/jsx-vanilla.git"
  },
  "homepage": "https://github.com/AdeonMaster/jsx-vanilla#readme",
  "bugs": {
    "url": "https://github.com/AdeonMaster/jsx-vanilla/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib --ignore src/__tests__/",
    "build": "npm run eslint && npm run build:lib && npm run jest",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "jest": "jest"
  },
  "keywords": [
    "html",
    "js",
    "preprocessor",
    "jsx",
    "acorn"
  ],
  "author": "AdeonMaster",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-imports": "^1.5.1",
    "eslint": "^5.16.0",
    "regenerator-runtime": "^0.13.2",
    "jest": "^24.8.0"
  },
  "dependencies": {
    "acorn": "^6.2.1",
    "acorn-jsx": "^5.0.1",
    "acorn-walk": "^6.2.0"
  }
}
